        {
          "$cmp": "FormKit",
          "props": {
            "type": "text",
            "name": "min",
            "label": "min",
            "validation": [["length:3,"], ["required"]]
          }
        },
        {
          "$cmp": "FormKit",
          "props": {
            "type": "text",
            "name": "max",
            "label": "max",
            "validation": [["length:0,3"], ["required"]]
