        {
          "$cmp": "FormKit",
          "props": {
            "type": "text",
            "name": "minmax",
            "label": "minmax",
            "validation": [["length:3,14"], ["required"]]
          }
        },
        {
          "$cmp": "FormKit",
          "props": {
            "type": "email",
            "name": "email",
            "label": "email",
            "validation": [["email"], ["required"]]
          }
        },
        {
          "$cmp": "FormKit",
          "props": {
            "type": "tel",
            "name": "telephone",
            "label": "telephone",
            "validation": [["required"]]
          }
        },
        {
          "$cmp": "FormKit",
          "props": {
            "type": "url",
            "name": "url",
            "label": "url",
            "validation": [["url"], ["required"]]
          }
        }
      ]
    }
  }
