{
    "openapi": "3.0.0",
    "info": {
      "title": "Simple API overview",
      "version": "1.0"
    },
    "paths": {
      "/number-examples": {
        "get": {
          "operationId": "listExamples",
          "summary": "List demo examples",
          "parameters": [
            {
              "in": "query",
              "name": "number",
              "required": true,
              "schema": {
                "type": "number"
              }
            },
            {
              "in": "query",
              "name": "number-min-zero",
              "required": true,
              "schema": {
                "type": "number",
                "minimum": 0
              }
            },
            {
              "in": "query",
              "name": "number-min",
              "required": true,
              "schema": {
                "type": "number",
                "minimum": 17
              }
            },
            {
              "in": "query",
              "name": "number-max",
              "required": true,
              "schema": {
                "type": "number",
                "maximum": 34
              }
            },
            {
              "in": "query",
              "name": "number-minmax",
              "required": true,
              "schema": {
                "type": "number",
                "minimum": 17,
                "maximum": 34
              }
            }
          ],
          "responses": {
            "200": {
              "description": "200 response",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "Example": {
          "description": "Example obj",
          "type": "object",
          "properties": {
            "name": { "type": "string" },
            "description": { "type": "string" }
          },
          "required": ["name", "description"]
        }
      }
    }
  }
  